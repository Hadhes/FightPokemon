        modifNomVS.textContent = "<%= pokemonsVS[5].nom.toUpperCase() %>";
        modifBarreVS.value = `${pvActuVS}`;
        modifBarreVS.max = "<%= pokemonsVS[5].stats.pv %>";
        modifPVMaxVS.textContent = `${pvActuVS}/<%= pokemonsVS[5].stats.pv %>`;
      }
    }
  </script>
